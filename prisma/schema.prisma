generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model paybutton_addresses {
  id          Int        @id @default(autoincrement())
  address     String     @db.VarChar(255)
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
  chainId     Int
  paybuttonId Int
  chains      chains     @relation(fields: [chainId], references: [id], onUpdate: Restrict, map: "paybutton_addresses_ibfk_1")
  paybuttons  paybuttons @relation(fields: [paybuttonId], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "paybutton_addresses_ibfk_2")

  @@index([chainId], map: "chainId")
  @@index([paybuttonId], map: "paybuttonId")
}

model paybuttons {
  id                  Int                   @id @default(autoincrement())
  providerUserId      String?               @db.VarChar(255)
  createdAt           DateTime              @default(now())
  updatedAt           DateTime              @updatedAt
  paybutton_addresses paybutton_addresses[]
}

model chains {
  id                  Int                   @id @default(autoincrement())
  slug                String                @db.VarChar(255)
  title               String                @db.VarChar(255)
  createdAt           DateTime              @default(now())
  updatedAt           DateTime              @updatedAt
  paybutton_addresses paybutton_addresses[]
}
